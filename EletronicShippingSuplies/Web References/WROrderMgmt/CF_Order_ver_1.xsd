      <xs:element minOccurs="0" name="GI" type="CF_GI:GenInfo">
        <xs:annotation>
          <xs:documentation>General Info</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="10" name="CDiv">
        <xs:annotation>
          <xs:documentation>Country division</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="xs:string">
              <xs:attribute name="DivTyCd">
                <xs:annotation>
                  <xs:documentation>Country division type code :Code that uniquely identifies a division type. 
				Valid values are:
A=Autonomous community
B=Autonomous country region
C=County
D=District
N=Canton
O=Country (should be used when no division type for a country has been identified)
P=Province
R=Country region
S=State.

This attribute was added as a hot fix for DCVS project in 042011.

It should be removed together with the other address attributes.

Referenced address entity should be used instead</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="35" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:attribute>
              <xs:attribute name="Nm" use="optional">
                <xs:annotation>
                  <xs:documentation>Country division name</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="45" />
                    <xs:minLength value="1" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:attribute>
              <xs:attribute name="Cd" use="optional">
                <xs:annotation>
                  <xs:documentation>Country diivision code</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:minLength value="1" />
                    <xs:maxLength value="5" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:attribute>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="SCCDev" type="CdmOrder_OrderCustomerCommunicationDevice">
        <xs:annotation>
          <xs:documentation>Ordercustomer detail has Order customer communication device </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="NtwAccNo">
      <xs:annotation>
        <xs:documentation>Network Account number : The account number identifier of the Account. This is the number that our customer is using.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="12" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="CntNm">
      <xs:annotation>
        <xs:documentation>Shipment customer contact name : The name of the person to contact in reference to a shipment.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="70" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="CustNm">
      <xs:annotation>
        <xs:documentation>Shipment customer name : The name of the person or company that is directly related to the shipment as a shipper, receiver, etc.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="100" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="Addr1">
      <xs:annotation>
        <xs:documentation>Shipment customer address line 1 : The first line of the shipment customer's address.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="45" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="Addr2">
      <xs:annotation>
        <xs:documentation>Shipment customer address line 2 : The second line of the shipment customer's address.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="45" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="Addr3">
      <xs:annotation>
        <xs:documentation>Shipment customer address line 3 : The third line of the shipment customer's address.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="45" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="Addr4">
      <xs:annotation>
        <xs:documentation>Shipment customer address line 4 : The fourth line of the shipment customer's address.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="45" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="AddrId">
      <xs:annotation>
        <xs:documentation>Address identifier : A unique number for identifying an Address.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="17" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="BsnPtyCd">
      <xs:annotation>
        <xs:documentation>Business party code : The unique identifier of a business party.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="20" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="CRlTyCd">
      <xs:annotation>
        <xs:documentation>Customer role type code : A code that uniquely identifies a customer role.
Values are:
BC=Barcode
BT=Bill To
BY=Buyer
DF=Destination Facility
DH=DDI Shipper
DP=DDI Payer
DR=DDI Receiver
DS=Dispatch
DL=Delivery
EP=Exporter
FR=Freight Customer
IN=Indivisible trans
IP=Importer
IQ=Inquirer
LP=Logistic Service Supplier
LS=Logistic Supplier Provider
MR=Message Receiver Party
MS=Message Sender Party
NF=Non Freight Customer
OB=Origin Barcode
OF=Origin Facility
OP=Operational
PK=Package key
PU=Pick-up
PY=Payer
RQ=Requestor
RT=Return To
RV=Receiver
SP=Shipper
SU=Supplier
TC=Technical
TH=Third party</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="2" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="CtryCd">
      <xs:annotation>
        <xs:documentation>/Country code : The country code is a two-character code that uniquely identifies a country.  The code should be aligned to the ISO (ISO 3166-1-alpha-2 code elements) or in certain circumstances created by DHL. 
Values are e.g.
BE=Belgium
FR=France
XC=Curacao (Not ISO - part of Netherlands Antilles)</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="2" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="CtyNm">
      <xs:annotation>
        <xs:documentation>/City name : City name is the full name of a city within a country.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="45" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="Dist">
      <xs:annotation>
        <xs:documentation>/Shipment customer district : A geographical area within a town or city.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="45" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="SCAttention">
      <xs:annotation>
        <xs:documentation>Shipment customer attention to : The name of the person that the shipment is attention to.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="70" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="SCBldNm">
      <xs:annotation>
        <xs:documentation>/Shipment customer building name : The name of the customer???s building, e.g. Kings House (not always indicative of the tenants or location).</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="45" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="SCRmk">
      <xs:annotation>
        <xs:documentation>Shipment customer remark : Provides any further information.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="250" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="StrNAddtn">
      <xs:annotation>
        <xs:documentation>/Shipment customer street number addition : Provides any additional information related to the street or house number.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="20" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="StrNm">
      <xs:annotation>
        <xs:documentation>/Shipment customer street name : The name of the customer???s street.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="45" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="StrNo">
      <xs:annotation>
        <xs:documentation>/Shipment customer street number : The number in the street that identifies the customer's site.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="20" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="VatNo">
      <xs:annotation>
        <xs:documentation>/Shipment customer VAT number : The customer's registration number.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="35" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="Zip">
      <xs:annotation>
        <xs:documentation>/Postcode : Postal Area is a code used within countries and country division to uniquely identify pickup and delivery areas used by the national postal organisation. Used by DHL to match customer known addresses into DHL's network of service area's.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="12" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="CdmOrder_OrderCustomerCommunicationDevice">
    <xs:annotation>
      <xs:documentation>List the communication devices that has been given for a shipment's customer. E.g. the telephone, mobile, fax or e-mail address of the shipper, receiver or a third parti.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
