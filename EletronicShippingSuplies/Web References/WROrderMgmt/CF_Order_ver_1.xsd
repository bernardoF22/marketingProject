            <xs:element minOccurs="0" name="Shp" type="CdmOrder_SUB_Shipment">
              <xs:annotation>
                <xs:documentation>Shipment</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:choice>
        </xs:complexType>
      </xs:element>
