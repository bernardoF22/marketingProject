    <xs:attribute name="PayTxChrg">
      <xs:annotation>
        <xs:documentation>Order line transaction tax charge in payer currency : The amount of tax that has been levied on this charge in payer currency.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:decimal">
