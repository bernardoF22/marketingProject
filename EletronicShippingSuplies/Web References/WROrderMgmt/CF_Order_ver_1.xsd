    <xs:attribute name="OrdNo" type="xs:string">
      <xs:annotation>
        <xs:documentation>Order number : A unique number identifying a particular order.
</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="BOVer">
      <xs:annotation>
        <xs:documentation>Version of business object.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="7" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="BOId">
      <xs:annotation>
        <xs:documentation>Id of business objects, this Id has to be unique.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:ID" />
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="AUId">
      <xs:annotation>
        <xs:documentation>Account unique identifier : A unique ten-digit number without leading zeros used to identify an account within DHL. The last digit is a check-digit calculated using the 'Modulus 7' method.

This will be allocated during migration of accounts into GCDB.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:decimal">
          <xs:totalDigits value="10" />
          <xs:fractionDigits value="0" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="BkgChnCd">
      <xs:annotation>
        <xs:documentation>Booking channel code : Identifies the booking channel used for the shipment.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="3" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="BkgDt">
      <xs:annotation>
        <xs:documentation>Booking date : The date when the courier needs to attend the customer premises in order to pick up the shipments.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:date" />
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="BkgRef">
      <xs:annotation>
        <xs:documentation>Booking reference : A reference that is used to associate an order with a booking.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="10" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="BkgSrcAppCd">
      <xs:annotation>
        <xs:documentation>Booking source application code : A code to uniquely define an application/system, e.g. 
DST
Eurion
GCA.
Network (In case a division has a network standard checkpoint/event structure this default code can be used)
</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="10" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="BkgTyCd">
      <xs:annotation>
        <xs:documentation>Booking type code : A code to uniquely identify a booking types used within the booking systems.

Valid types are
A = Adhoc Advance 
S = Adhoc Same day or Special 
R = Regular
U = Unknown</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="1" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="CrDtm">
      <xs:annotation>
        <xs:documentation>Order creation datetime : Indicates the date and time for creation of the order.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:dateTime" />
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="OrdTyCd">
      <xs:annotation>
        <xs:documentation>Order type code : Identifies uniquely an order type.
BLK=Bulk Pick Up
NEW=New Purchase Order
RPR=Repair Purchase Order
SHP=Shipment</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="3" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="RMANo">
      <xs:annotation>
        <xs:documentation>Order RMA number : A number provided by the customer to uniquely identify a Repair and return order.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="40" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="RtLbCd">
      <xs:annotation>
        <xs:documentation>Order return label code : Code to identify if a good or defective part is returned and to which address to send it.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="5" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="SrcRef">
      <xs:annotation>
        <xs:documentation>Order source reference : A source specific identifier for a logical grouping of services provided or to be provided by DHL in response to a customer request.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="20" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="SrvRqNo">
      <xs:annotation>
        <xs:documentation>Service request number : A unique number identifying a particular request or ticket.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:decimal">
          <xs:totalDigits value="15" />
          <xs:fractionDigits value="0" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="Pri">
      <xs:annotation>
        <xs:documentation>Priority: "Defines in which priority the handling instruction should be processed:?? 10 Low?? 50 Medium?? 70 HighThe values are 0 to 100. The reserved valuefor Low is 10, for Medium is 50, and for High is 70. Other priorities MAY be added but MUST NOT exceed 100."</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
