            <xs:element minOccurs="0" name="BulkPickUp" type="CdmOrder_SUB_BulkPickUp">
              <xs:annotation>
                <xs:documentation>Bulk pick up</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="CmrcJdg" type="CdmOrder_SUB_CommercialJudgement">
              <xs:annotation>
                <xs:documentation>Commercial judgement</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="CstChrg" type="CdmOrder_SUB_CustomsCharge">
              <xs:annotation>
                <xs:documentation>Customs charge</xs:documentation>
              </xs:annotation>
